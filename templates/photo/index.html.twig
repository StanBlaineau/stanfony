{% extends 'base.html.twig' %}

{% block title %}Photo{% endblock %}

{% block content %}
    <h1>Photos</h1>

    <div class="container">
        <div class="row">
            {% for photo in photos %}
                <div class="col-2">
                    <img src="{{ photo.download_url }}" alt="" class="img-fluid" />
                </div>
            {% endfor %}
        </div>
    </div>
{% endblock %}
